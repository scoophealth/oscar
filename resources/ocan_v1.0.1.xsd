	<xsd:element name="domain">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence>

						<!-- common for all domains -->
						<xsd:element ref="ocan:needRating" minOccurs="0"/>
						<xsd:element ref="ocan:informalHelpRecvd" minOccurs="0"/>
						<xsd:element ref="ocan:formalHelpRecvd" minOccurs="0"/>
						<xsd:element ref="ocan:formalHelpNeed" minOccurs="0"/>

						<!-- 01: accommodation -->
						<xsd:element ref="ocan:residenceType" minOccurs="0"/>
						<xsd:element ref="ocan:residenceSupport" minOccurs="0"/>
						<xsd:element ref="ocan:livingArrangementType" minOccurs="0"/>

						<!-- 05: daytime activities -->
						<xsd:element ref="ocan:employStatus" minOccurs="0"/>
						<xsd:element ref="ocan:educationProgramStatus" minOccurs="0"/>
						<xsd:element ref="ocan:riskUnemploymentList" minOccurs="0"/>

						<!-- 06: physical health -->
						<xsd:element ref="ocan:medicalConditionList" minOccurs="0"/>
						<xsd:element ref="ocan:physicalHealthConcern" minOccurs="0"/>
						<xsd:element ref="ocan:concernAreaList" minOccurs="0"/>
						<xsd:element ref="ocan:medicationList" minOccurs="0"/>
						<xsd:element ref="ocan:sideEffects" minOccurs="0"/>
						<xsd:element ref="ocan:dailyLivingAffected" minOccurs="0"/>
						<xsd:element ref="ocan:sideEffectsDetailList" minOccurs="0"/>

						<!-- 07: psychotic symptoms -->
						<xsd:element ref="ocan:hospitalizedPastTwoYears" minOccurs="0"/>
						<xsd:element ref="ocan:totalAdmissions" minOccurs="0"/>
						<xsd:element ref="ocan:totalHospitalDays" minOccurs="0"/>
						<xsd:element ref="ocan:communityTreatOrder" minOccurs="0"/>
						<xsd:element ref="ocan:symptomList" minOccurs="0"/>

						<!-- 08: condition and treatment -->
						<xsd:element ref="ocan:diagnosticList" minOccurs="0"/>
						<xsd:element ref="ocan:otherIllnessList" minOccurs="0"/>

						<!-- 10: safety to self -->
						<xsd:element ref="ocan:suicideAttempt" minOccurs="0"/>
						<xsd:element ref="ocan:suicideThoughts" minOccurs="0"/>
						<xsd:element ref="ocan:safetyConcernSelf" minOccurs="0"/>
						<xsd:element ref="ocan:safetyToSelfRiskList" minOccurs="0"/>

						<!-- 12: alcohol -->
						<xsd:element ref="ocan:drinkAlcohol" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeAlcohol" minOccurs="0"/>

						<!-- 13: drugs -->
						<xsd:element ref="ocan:drugUseList" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeDrugs" minOccurs="0"/>

						<!-- 14: other addictions -->
						<xsd:element ref="ocan:addictionTypeList" minOccurs="0"/>
						<xsd:element ref="ocan:stageOfChangeAddictions" minOccurs="0"/>

						<!-- 15: company -->
						<xsd:element ref="ocan:changedSocialPatterns" minOccurs="0"/>

						<!-- 20: basic education -->
						<xsd:element ref="ocan:highestEducationLevel" minOccurs="0"/>

						<!-- 23: money -->
						<xsd:element ref="ocan:sourceOfIncome" minOccurs="0"/>

					</xsd:sequence>

					<xsd:attribute name="name" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="01-accommodation"/>
								<xsd:enumeration value="02-food"/>
								<xsd:enumeration value="03-looking after the home"/>
								<xsd:enumeration value="04-self-care"/>
								<xsd:enumeration value="05-daytime activities"/>
								<xsd:enumeration value="06-physical health"/>
								<xsd:enumeration value="07-psychotic symptoms"/>
								<xsd:enumeration value="08-condition and treatment"/>
								<xsd:enumeration value="09-psychological distress"/>
								<xsd:enumeration value="10-safety to self"/>
								<xsd:enumeration value="11-safety to others"/>
								<xsd:enumeration value="12-alcohol"/>
								<xsd:enumeration value="13-drugs"/>
								<xsd:enumeration value="14-other addictions"/>
								<xsd:enumeration value="15-company"/>
								<xsd:enumeration value="16-intimate relationships"/>
								<xsd:enumeration value="17-sexual expression"/>
								<xsd:enumeration value="18-childcare"/>
								<xsd:enumeration value="19-other dependents"/>
								<xsd:enumeration value="20-basic education"/>
								<xsd:enumeration value="21-telephone"/>
								<xsd:enumeration value="22-transport"/>
								<xsd:enumeration value="23-money"/>
								<xsd:enumeration value="24-benefits"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- common for all domains -->
	<xsd:element name="needRating">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">

					<xsd:sequence/>

					<xsd:attribute name="staff" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:byte">
								<xsd:enumeration value="0"/>	<!-- No problem -->
								<xsd:enumeration value="1"/>	<!-- No or moderate problem due to help given -->
								<xsd:enumeration value="2"/>	<!-- Serious problem -->
								<xsd:enumeration value="9"/>	<!-- Not known -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

					<xsd:attribute name="client" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:byte">
								<xsd:enumeration value="-1"/>	<!-- Blank -->
								<xsd:enumeration value="0"/>	<!-- No problem -->
								<xsd:enumeration value="1"/>	<!-- No or moderate problem due to help given -->
								<xsd:enumeration value="2"/>	<!-- Serious problem -->
								<xsd:enumeration value="9"/>	<!-- Not known -->
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!-- common for all domains -->
	<xsd:element name="informalHelpRecvd">
