								<xsd:enumeration value="1"/>	<!-- Low Help -->
								<xsd:enumeration value="2"/>	<!-- Moderate Help -->
								<xsd:enumeration value="3"/>	<!-- High Help -->
								<xsd:enumeration value="9"/>	<!-- Not known -->
