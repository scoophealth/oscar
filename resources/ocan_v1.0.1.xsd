				<xsd:enumeration value="50705009"/>	<!-- Factitious Disorders  -->
				<xsd:enumeration value="66347000"/>	<!-- Impulse Control Disorders not elsewhere classified -->
				<xsd:enumeration value="MDGMC"/>		<!-- Mental Disorders due to General Medical Conditions -->
				<xsd:enumeration value="46206005"/>	<!-- Mood Disorder -->
				<xsd:enumeration value="33449004"/>	<!-- Personality Disorders -->
				<xsd:enumeration value="58214004"/>	<!-- Schizophrenia other Psychotic Disorders -->
				<xsd:enumeration value="39898005"/>	<!-- Sexual and Gender Identity Disorders -->
				<xsd:enumeration value="31297008"/>	<!-- Sleep Disorders -->
				<xsd:enumeration value="87858002"/>	<!-- Somatoform Disorders  -->
				<xsd:enumeration value="SRDDH"/>		<!-- Substance Related Disorders, Developmental Handicap -->
				<xsd:enumeration value="DH"/>			<!-- Developmental Handicap -->
				<xsd:enumeration value="261665006"/>	<!-- Unknown -->
				<xsd:enumeration value="CDA"/>			<!-- Client declined to answer -->
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>

	<!-- 08: condition and treatment: Other Illness Information (check all that apply) -->
	<xsd:element name="otherIllnessList">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="xsd:anyType">
					<xsd:sequence>
						<xsd:element ref="ocan:otherIllness" maxOccurs="unbounded" minOccurs="0"/>
					</xsd:sequence>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="otherIllness">
		<xsd:simpleType>
			<xsd:restriction base="xsd:string">
				<xsd:enumeration value=""/>		<!-- Null -->
				<xsd:enumeration value="016A-01"/>	<!-- Concurrent Disorder (Substance Abuse) -->
				<xsd:enumeration value="016A-02"/>	<!-- Dual Diagnosis (Development Disability) -->
				<xsd:enumeration value="016A-03"/>	<!-- Other Chronic Illness and,or physical disabilities -->
			</xsd:restriction>
