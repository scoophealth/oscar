				<xsd:enumeration value="018-06"/>	<!-- Family Physicians -->
				<xsd:enumeration value="018-07"/>	<!-- Psychiatrists -->
				<xsd:enumeration value="018-08"/>	<!-- Mental Health Worker -->
				<xsd:enumeration value="018-09"/>	<!-- Criminal Justice System -->
				<xsd:enumeration value="018-10"/>	<!-- Self, Family or Friend -->
				<xsd:enumeration value="018-11"/>	<!-- Other -->
			</xsd:restriction>
		</xsd:simpleType>
