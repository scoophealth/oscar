			<xsd:restriction base="xsd:string">
				<xsd:enumeration value=""/>		<!-- Null -->
				<xsd:enumeration value="TRUE"/>	<!-- Yes -->
				<xsd:enumeration value="FALSE"/>	<!-- No -->
				<xsd:enumeration value="UNK"/>		<!-- Unknown -->
				<xsd:enumeration value="CDA"/>		<!-- Client declined to answer -->
