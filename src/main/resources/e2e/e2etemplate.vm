						<templateId root="2.16.840.1.113883.3.1818.10.3.15"/>
						<!-- Problem List Observation -->
						<observation classCode="OBS" moodCode="EVN">
							<id extension="$problem.id" root="2.16.840.1.113883.3.1818.10.2.10.31"/>
