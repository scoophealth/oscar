								<time value="$dateTool.format('yyyyMMdd', $problem.getUpdateDate())"/>
#*        *##else
								<time nullFlavor="UNK"/>
#*        *##end
								<assignedAuthor>
									<id use="BUS" extension="38809" root="2.16.840.1.113883.3.163.5.0.10.2"/>
									<assignedPerson classCode="PSN" determinerCode="INSTANCE">
