<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.PMmodule.model">
	<class name="IntakeNodeJavascript" table="intake_node_js">
		<meta attribute="sync-DAO">false</meta>

		<id name="id" type="integer" column="id">
			<generator class="native" />
		</id>

		<property name="questionId" column="question_id" type="string" not-null="true"/>
		
		<property name="location" column="location" type="string" not-null="true"/>

	</class>
</hibernate-mapping>
