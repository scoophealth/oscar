<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="org.oscarehr.casemgmt.model.CaseManagementNoteExt" table="casemgmt_note_ext">
		<id name="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property column="note_id" name="noteId" />
		<property column="key_val" name="keyVal" />
		<property column="value" name="value" />
		<property column="date_value" name="dateValue" type="java.util.Date" />
	</class>
</hibernate-mapping>
