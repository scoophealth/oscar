<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD //EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.caisi.model.CaisiEditor" table="caisi_editor">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="increment" />
		</id>		
		<property name="category" column="category" type="string" length="50" not-null="true" />
		<property name="label" column="label" type="string" length="255" not-null="true" />
		<property name="type" column="type" type="string" length="10" not-null="true" />
		<property name="labelValue" column="labelValue" type="string" length="255" not-null="false" />
		<property name="labelCode" column="labelCode" type="string" length="50" not-null="false" />
		<property name="horizontal" column="horizontal" type="string" length="3" not-null="false" />
		<property name="isActive" column="isActive" type="string" length="3" not-null="false" />
	</class>
</hibernate-mapping>
