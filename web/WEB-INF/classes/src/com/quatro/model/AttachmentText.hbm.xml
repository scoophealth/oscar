<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.quatro.model.AttachmentText" table="attachment_text" mutable="true">
		<id name="docId" />
        <property name="revDate" column="revdatetime"/> 
		<property name="attDataBlob" type="blob" column="docdata" />
	</class>
</hibernate-mapping>

