<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.quatro.model.LookupTableDefValue" table="app_lookuptable" mutable="false">
		<id name="tableId" unsaved-value="0">
			<generator class="native" />
		</id>
        <property name="tableName" column="table_name" /> 
        <property name="description" /> 
        <property name="active" column="activeyn" /> 
		<property name="tree" column="istree" />
        <property name="treeCodeLength" column="treecode_length" />
        <property name="moduleId"/>
        <property name="moduleName"  formula="(select t.description from v_lookup_table t where t.prefix='FCT' and t.code=moduleId)"/> 
	</class>
</hibernate-mapping>
